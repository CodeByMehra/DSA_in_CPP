//Array: it can be deined as a type of data structure that consist a collection of similiar data types

#include <iostream>
using namespace std;

int main() {
    // Declare an array of integers
    int arr[5];

    //taking input using loop
    for (int i=0; i<=5; i++){
        cin>>arr[i];
    }

    for (int i=0; i<=5; i++){
        cout<<arr[i];
    }
}